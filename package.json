{
  "name": "green-analytics-js",
  "version": "1.0.16",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:MakakWasTaken/green-analytics-js.git",
  "author": "Markus Moltke <markus@green-analytics.com>",
  "license": "MPL-2.0",
  "scripts": {
    "build": "tsc -b && yarn browser:build",
    "browser:build": "browserify build/index.js > build/bundle.js",
    "check": "npm run check:tsc",
    "check:tsc": "tsc -p . --noEmit",
    "lint": "biome format --write . && biome lint --apply . --no-errors-on-unmatched",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.4",
    "browserify": "^17.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}