{
  "name": "green-analytics-js",
  "version": "1.0.13",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:MakakWasTaken/green-analytics-js.git",
  "author": "Markus Moltke <markus@green-analytics.com>",
  "license": "MPL",
  "scripts": {
    "build": "tsc -b",
    "check": "npm run check:tsc",
    "check:tsc": "tsc -p . --noEmit",
    "lint": "biome format --write . && biome lint --apply .",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "biome format --write",
    "*.{js,jsx,ts,tsx|json}": "biome lint --apply"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.9.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}